<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Results</servlet-name>
        <servlet-class>ru.voskhod.tik.Results</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Protocol</servlet-name>
        <servlet-class>ru.voskhod.tik.Protocol</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Summary</servlet-name>
        <servlet-class>ru.voskhod.tik.Summary</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Campaigns</servlet-name>
        <servlet-class>ru.voskhod.tik.Campaigns</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SideBar</servlet-name>
        <servlet-class>ru.voskhod.tik.SideBar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestServlet</servlet-name>
        <servlet-class>ru.voskhod.tik.TestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Admin</servlet-name>
        <servlet-class>ru.voskhod.tik.Admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TotalResults</servlet-name>
        <servlet-class>ru.voskhod.tik.TotalResults</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Results</servlet-name>
        <url-pattern>/Results</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Protocol</servlet-name>
        <url-pattern>/Protocol</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Summary</servlet-name>
        <url-pattern>/Summary</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Campaigns</servlet-name>
        <url-pattern>/Campaigns</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SideBar</servlet-name>
        <url-pattern>/SideBar</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>exchange</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>exchange</servlet-name>
        <!--<url-pattern>*.htm</url-pattern>-->
        <url-pattern>/Exchange</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestServlet</servlet-name>
        <url-pattern>/TestServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Admin</servlet-name>
        <url-pattern>/Admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TotalResults</servlet-name>
        <url-pattern>/TotalResults</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <error-page>
        <exception-type>java.lang.Throwable</exception-type>
        <location>/jsp/error.jsp</location>
    </error-page>
</web-app>
